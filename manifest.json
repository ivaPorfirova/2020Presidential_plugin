{
  "manifest_version": 2,
  "name": "Google Chrome URL Logger",
  "version": "1.0",

  "permissions": ["storage"],
  "browser_action": {
        "default_popup": "popup.html"
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["contentScript.js"]
    }
  ]
}
